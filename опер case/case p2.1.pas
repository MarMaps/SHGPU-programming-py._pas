program op_case_1;
  var x:integer;
  
begin
  writeln ('Введите число от 1 до 10');
  readln (x);
  case x of
    1: writeln ('I');
    2: writeln ('II');
    3: writeln ('III');
    4: writeln ('IV');
    5: writeln ('V');
    6: writeln ('VI');
    7: writeln ('VII');
    8: writeln ('VIII');
    9: writeln ('IX');
    10: writeln ('X');
  end;
end.
